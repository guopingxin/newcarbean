
.container{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.3);
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
}

.bottom{
  position: fixed;
  bottom: 0;
  width: 100%;
  background: white;
  text-align: center;
}

.phone{
  color: rgb(170, 168, 168);
  padding: 20rpx 0;
  border-bottom: 1px solid #eee;
}

.call{
  padding: 20rpx 0;
}

.cancel{
  border-top: 4px solid #eee;
}