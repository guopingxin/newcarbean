.eorder{
  width: 100%;
  padding: 2% 5%;
  border-radius: 10rpx;
  border: 1rpx solid rgb(241, 241, 241);
  margin-bottom: 14rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  color: #979797;
}

.etitle-estate{
  display: flex;
  justify-content: space-between;
  margin-bottom: 10rpx;
  color: #1a1a1d;
}

.estate{
  display: flex;
  align-items: center;
}

.etitle-estate image{
  width: 26rpx;
  height: 22rpx;
}

.e-startaddress{
  display: flex;
  margin-top: 10rpx;
  align-items: center;
}

.e-startaddress image{
  width: 26rpx;
  height: 30rpx;
  margin-right: 14rpx;
}