.search-box {
  width: 702rpx;
  height: 72rpx;
  line-height: 72rpx;
  text-align: center;
  margin: 24rpx auto;
  background: #EDEDED;
  border-radius: 36px;
  color: #8E8E93;
}

.goodservicetext{
  font-weight: 600;
  font-size: 20px;
  margin: 3%;
}

.goodlist{
  display: flex;
  height: 280rpx;
  width: 94%;
  margin: 0 3%;  
  justify-content:space-between;
}

.goodimg{
  width: 48.5%;
  height: 100%;
  display: flex;
  flex-direction: column-reverse;
  align-items:center;
}

.white-mode{
  width: 96%;
  height: 50%;
  background: url(http://www.feecgo.com/level/white-mode.png) no-repeat left top;
  background-size: 100% 100%;
  display: flex;
  flex-direction: column;
  
}

.goodtitle{
  margin: 5rpx 14rpx 0 14rpx;
  font-family: "PingFangSC-Medium";
  font-size: 36rpx;
}

.goodtips{
  margin: 5rpx 14rpx 0 14rpx;
  font-family: "PingFangSC-Regular";
  font-size: 26rpx;
}


.goodserviceimg{
  width: 45.5%; 
  height: 280rpx;
}

.imgpad1{
  margin: 0 1.5% 0 3%
}

.imgpad2{
  margin: 0 3% 0 1.5%
}

.choose_cell{
  margin: 0 5%;
  display: flex;
}

.each_choose_cell{
  width: 33.33%;
  line-height: 50px;
}

.area{
  display: inline-block;
  font-size: 12px;
}

.down-img{
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
}

.noservice{
  width: 100%;
  text-align: center;
  margin-top: 22rpx;
}