<!--pages/edaijia/fee/fee.wxml-->
<view class="container">
  <view class="money">
    <view>约</view>
    <view class="moneyfont">
    {{cash}}
    </view>
    <view>元</view>
  </view>
  <view class="money1">日常代驾费用明细</view>
  <view class="fee-item">
    <view>开始时间</view>
    <view>{{startTime}}</view>
  </view>
  <view class="fee-item">
    <view>结束时间</view>
    <view>{{endTime}}</view>
  </view>
  <view class="fee-item">
    <view>总里程费</view>
    <view>{{fee.collectionFeeParse.totalMileageFee.amount}}元</view>
  </view>
  <view class="fee-item">
    <view>贵宾卡抵扣</view>
    <view>{{fee.couponDetail.money}}元</view>
  </view>
  <view class="fee-item">
    <view>现金支付</view>
    <view>{{fee.income}}元</view>
  </view>
</view>
