/* pages/common/member/examine/examineloaction/examinelocation.wxss */
.search-box {
  width: 702rpx;
  height: 72rpx;
  line-height: 72rpx;
  text-align: center;
  margin: 24rpx auto;
  background: #EDEDED;
  border-radius: 36px;
  color: #8E8E93;
}

.address-list {
  width: 100%;
  margin: 0 auto;
  display: flex;
  flex-direction: column;
  padding-bottom: 104rpx
}

.address-item {
  width: 100%;
  height: 80rpx;
  padding: 0 24rpx;
  border-bottom: 1px solid #eee;
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.address-item image {
  width: 48rpx;
  height: 48rpx;
}

.address-btn {
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  background: #1a65ff;
  position: fixed;
  bottom: 0;
  left: 0;
  font-size: 36rpx;
}