/* 账户明细 */
.container {

}
.section {

}
.bean-cell {
  padding: 0 24rpx;
}
.bean-cell>text {
  font-size: 36rpx;
}

.nodata{
  text-align: center;
  width: 100%;
  padding: 24rpx 0;
  font-size: 20rpx;

}
.bean-item {
  width: 100%;
  height: 120rpx;
  padding-left: 10rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  margin-top: 12rpx;
}
.bean-left {
  display: flex;
  align-items: center;
}
.bean-img {
  position: relative;
  margin-right: 24rpx;
}
.bean-img image {
  width: 60rpx;
  height: 60rpx;
}

.dot{
  width: 10rpx;
  height: 10rpx;
  background: #B4B4B4;
  border-radius: 50%;
  position: absolute;
  left: 27rpx;
  z-index: 100;
  top: -30rpx;
}

.line {
  width: 2rpx;
  height: 132rpx;
  position: absolute;
  z-index: -2;
  background: #26272C;
  opacity: 0.2;
  left: 29rpx;
  top: -30rpx;

}
.bean-content {
  display: flex;
  flex-direction: column;
}
.bean-content text:first-child {

}
.bean-content text:last-child {
  color: #B4B4B4;
  font-size: 24rpx;
}
.bean-right {
  font-size: 36rpx;
}