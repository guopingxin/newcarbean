/* pages/community/community.wxss */

@import '../../components/community-modal/community.wxss';

.container{
  width: 100%;
}

.category{
  width: 86%;
  margin: 4% 7%;
  display: flex;
  justify-content: space-between;
}

.baoxian{
  display: flex;
  flex-direction: column;
  align-items: center;
}

.baoxian image{
  width: 100rpx;
  height: 100rpx;
  margin-bottom: 10rpx;
}

.swiper{
  width: 100%;
  display: flex;
  justify-content: center;
  height: 400rpx;
}

.swiper-item{
  padding: 0 2%;
  width: 80% !important;
  margin-left: 8% !important;
  display: flex;
  align-items: center;
}

.slide-image{
  width: 100%;
  height: 400rpx;
  border-radius: 20rpx;
}

.slide-image1{
  width: 100%;
  height: 300rpx;
  border-radius: 20rpx;
}

.communitytitle{
  display: flex;
  width: 100%;
}

.com{
  width: 25%;
  text-align: center;
  font-size: 20px;
  padding-bottom: 6rpx;

}

.title1{
  width: 25%;
  border-bottom: 3px solid #000;
  color: #000;
}

.title2{
  color: #a1a1a1
}

.title3{
  color: #a1a1a1
}

.title4{
  color: #a1a1a1
}

.section{
  /* margin-top: 80rpx; */
}

.change_cell_nomap {
  display: inline-block;
  width: 225px;
  background: #fff;
  border-radius: 20px;
  font-size: 15px;
  color: #a1a1a1;
  padding: 2px;
  border: 1rpx solid #1a84ff;
}

.change_cell_nomap .each_change {
  display: inline-block;
  width: 50%;
  line-height: 28px;
}

.change_cell_nomap .active {
  background: #1a84ff;
  color: #fff;
  border-radius: 20px;
}

.outMap .top {
  background: #f2f2f2;
  z-index: 3;
}

.outMap .setting {
  float: right;
  width: 36px;
  height: 36px;
  margin-right: 18px;
}

.outMap .setting image {
  width: 36px;
  height: 36px;
}

.each_dynamic {
  
  border-bottom: 6px solid #f2f2f2;
}

.public {
  position: fixed;
  bottom: 30px;
  right: 7px;
  z-index: 10;
}

.top1{
  position: fixed;
  top: 2%;
  z-index: 1000;
  background: transparent;
  height: 10%;
  width: 100%;
}

.public cover-image {
  width: 64px;
  height: 64px;
}

.public_modal {
  position: fixed;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100vh;
  background: rgba(0, 0, 0, 0.24);
  z-index: 200;
}

.pmodal_content {
  position: fixed;
  width: 100%;
  bottom: 0px;
  left: 0px;
  background: #fff;
  z-index: 200;
  padding: 22px 0px;
}

.icons {
  display: flex;
  justify-content: space-around;
}

.icon_cell cover-image {
  display: inline-block;
  width: 112rpx;
  height: 170rpx;
}

.close {
  margin-top: 20px;
  width: 44%;
  height: 80rpx;
  margin-left: 28%;
}

.map{
  position: fixed;
  top: 0;
  left: 0;
  width:100%;
  z-index: 10;
  margin-top: 70rpx;
  height: 100%;
}

/* .section{

  margin-top: 12%;
} */

.scale_icon_add{
  width: 80rpx;
  height: 90rpx;
  position: fixed;
  top: 500rpx;
  right: 30rpx;
  z-index: 10
}

.scale_icon_decrease{
  width: 80rpx;
  height: 90rpx;
  position: fixed;
  top: 588rpx;
  right: 30rpx;
  z-index: 10
} 

.setting{
  width: 80rpx;
  height: 80rpx;
  position: fixed;
  top: 720rpx;
  right: 30rpx;
  z-index: 10
}

.setshow{
  position: fixed;
  top: 500rpx;
  right: 30rpx;
  z-index: 100;
}

.swipermode{
  width: 100%;
  height: 220px;
  box-sizing: border-box;
}
swiper{
  height: 220px;
  width: 100%;
  display: flex;
  align-items: center;
}
.content{
  font-size: 16px;
  /* padding: 20rpx 40rpx; */
  position:absolute;
  bottom: 0;
  height: 90rpx;
  background: rgba(25, 25, 25, 0.5);
  color: white;
  display: flex;
  align-items: center;
  width: 100%;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
  padding-left: 20rpx;
}
swiper-item{
  text-align: center;
  display: flex;
  align-items: center;
}
swiper-item image{
  width: 100%;
  height: 200px;
  border-radius: 20rpx;
}
.box{
  width: 96%;
  height:100%;
  display: flex;
  /* margin-top: 40rpx; */
  box-sizing: border-box;
  box-shadow: 0 0 4rpx 4rpx #f2f2f2;
  position:relative;
  /* top:35%; */
  transform:translateY(0);
  flex-direction: column;
  justify-content: center;
  border-radius: 20rpx;
  left: 14rpx;
}

.public_content1{
  position: fixed;
  width: 100%;
  bottom: 0px;
  left: 0px;
  background: #fff;
  z-index: 200;
  height: 430rpx;
  border-top-left-radius: 22rpx;
  border-top-right-radius: 22rpx;
}

.fabu_whole{
  width: 65%;
  padding: 0 17.5%;
  display: flex;
  height: 100%;
  flex-direction: column;
  justify-content: center;
}

.fabu_style{
  display: flex;
  height: 160rpx;
  justify-content: space-between;
  width: 100%;
  align-items: center;
}

.fabu_style1{
  display: flex;
  height: 160rpx;
  width: 100%;
  align-items: center;
}

.fabu_item{
  display: flex;
  flex-direction: column;
  align-items: center;
}

.fabu_item cover-image{
  width: 80rpx;
  height: 80rpx;
  margin-bottom: 10rpx;
}

.fabu_close{
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  width: 30rpx;
  height: 30rpx;
}




