/* 河南中银的重新绑定手机号 */

page {
  height: 100%;
}

.change-body {
  height: 100%;
}

.change-title {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  padding: 12rpx;
}

.change-title image {
  width: 90rpx;
  height: 115rpx;
  margin-bottom: 12rpx;
}

.change-title text {
  color: #040405;
}

.change-content {
  padding: 24rpx;
}

.change-item {
  height: 90rpx;
  box-shadow: 0 0 6rpx 0 rgba(0, 0, 0, 0.2);
  border-radius: 60rpx;
  display: flex;
  align-items: center;
  padding: 0 34rpx;
  margin-bottom: 48rpx;
}

.change-item image {
  width: 50rpx;
  height: 50rpx;
}

.line {
  width: 0;
  height: 50rpx;
  border-left: 1rpx solid #d0d0d0;
  margin: 0 48rpx 0 38rpx;
}

.change-input {
}

.code-input {
  width: 240rpx;
}

.code-text {
  width: 180rpx;
  height: 40rpx;
  color: #1a84ff;
  display: flex;
  align-items: center;
  justify-content: flex-end;
}

.change-btn {
  width: 630rpx;
  height: 90rpx;
  background: #1a65ff;
  border-radius: 60rpx;
  margin-top: 64rpx;
  font-size: 36rpx;
  color: #fff;
  display: flex;
  align-items: center;
  justify-content: center;
}
